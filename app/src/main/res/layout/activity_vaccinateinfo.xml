<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/bg_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:visibility="gone">

                <ImageView
                    android:id="@+id/vaccinate0"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:background="#FFFFFF"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate0" />

                <ImageView
                    android:id="@+id/vaccinate1"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate1" />
                <ImageView
                    android:id="@+id/vaccinate2"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate2" />
                <ImageView
                    android:id="@+id/vaccinate3"
                    android:layout_width="wrap_content"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate3" />
                <ImageView
                    android:id="@+id/vaccinate4"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:background="#FFFFFF"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate4" />

                <ImageView
                    android:id="@+id/vaccinate5"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate5" />
                <ImageView
                    android:id="@+id/vaccinate6"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate6" />
                <ImageView
                    android:id="@+id/vaccinate7"
                    android:layout_width="wrap_content"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate7" />
                <ImageView
                    android:id="@+id/vaccinate8"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:background="#FFFFFF"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate8" />

                <ImageView
                    android:id="@+id/vaccinate9"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate9" />
                <ImageView
                    android:id="@+id/vaccinate10"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate10" />
                <ImageView
                    android:id="@+id/vaccinate11"
                    android:layout_width="wrap_content"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate11" />
                <ImageView
                    android:id="@+id/vaccinate12"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:background="#FFFFFF"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate12" />

                <ImageView
                    android:id="@+id/vaccinate13"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate13" />
                <ImageView
                    android:id="@+id/vaccinate14"
                    android:layout_width="match_parent"
                    android:layout_height="420dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/vaccinate14" />

            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <Button
        android:id="@+id/btn_vaccine_webview"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="29dp"
        android:background="@drawable/plus"
        android:textColor="@color/fr_drakGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_vaccine_webview2" />

    <TextView
        android:id="@+id/btn_vaccine_webview2"
        android:layout_width="74dp"
        android:layout_height="36dp"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="6dp"
        android:background="@drawable/bg_gray_with_bubbleborder"
        android:gravity="center"
        android:text="더 많은 정보 \n알아보기"
        android:textColor="@color/fr_drakGray"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_vaccine_webview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/btn_vaccine_webview"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>